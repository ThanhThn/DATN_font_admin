{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@pages/*": [
        "./pages/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "store/user/useUserLoadMoreStore",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
