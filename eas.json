{
  "cli": {
    "version": ">= 15.0.10",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType" : "apk"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "AES_IV": "$AES_IV",
        "AES_KEY": "$AES_KEY",
        "API_ADMIN_URL": "$API_ADMIN_URL",
        "API_DATA_URL": "$API_DATA_URL",
        "KEY_TOKEN": "$KEY_TOKEN",
        "TIMEZONE": "$TIMEZONE"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
